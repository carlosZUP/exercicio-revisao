package br.com.zup.ex4;

import java.util.Scanner;

public class Exercicio4 {
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		System.out.println("Digite o nome do candidato:");
		String nomeCandidato = entrada.next();
		
		System.out.println("Qual o numero do canditado:");
		int numeroCandidato = entrada.nextInt();
			
		 for (int voto = 0; voto <= 20; voto++) {
			System.out.println("Digite o numero do seu candidato");
		}
		 System.out.println("Voto adicionado para o candidato " + nomeCandidato);
		
		
	}
	
}
